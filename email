import csv
import os

csvpath = os.path.join('employees.csv')

with open('emplyees', 'w', newline='') as csvfile:
    reader
    for row in reader: csv.Dictreader(csvfile)
        first_name = row["first_name"]
        last_name = row["last_name"]
        email = f"{first_name}.{last_name}@exammple.com"